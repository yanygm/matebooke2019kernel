	cam_indicator_en: cam-indicator-en-state {
		pins = "gpio12";
		function = "gpio";

		drive-strength = <2>;
		bias-disable;
	};

	sn65dsi86_pin_active: sn65dsi86-enable-state {
		pins = "gpio96";
		function = "gpio";

		drive-strength = <2>;
		bias-disable;
	};

