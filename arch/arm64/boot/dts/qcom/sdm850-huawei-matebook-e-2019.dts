		pins = "gpio125";
		function = "gpio";

		input-enable;
		bias-pull-up;
		drive-strength = <2>;
	};

	mode_pin_active: mode-pin-state {
		pins = "gpio79";
		function = "gpio";

		bias-disable;
	};

	ec_int_default: ec-int-default-state {
		pins = "gpio24";
		function = "gpio";
		
		drive-strength = <2>;
		bias-disable;
	};

	pcie0_default: pcie0-default-state {
		clkreq-pins {
			pins = "gpio36";
			function = "pci_e0";
			bias-pull-up;
		};

		reset-n-pins {
			pins = "gpio35";
			function = "gpio";

			drive-strength = <2>;
			output-low;
			bias-pull-down;
		};

		wake-n-pins {
		    pins = "gpio37";
		    function = "gpio";

			drive-strength = <2>;
		    bias-pull-up;
	    };
	};

	pcie0_pwren_state: pcie0-pwren-state {
		pins = "gpio90";
		function = "gpio";

		drive-strength = <2>;
		bias-disable;
	};

	pcie1_default: pcie1-default-state {
		perst-n-pins {
			pins = "gpio102";
			function = "gpio";

			drive-strength = <16>;
			bias-disable;
		};

		clkreq-pins {
			pins = "gpio103";
			function = "pci_e1";
			bias-pull-up;
		};

		reset-n-pins {
			pins = "gpio113";
			function = "gpio";

			drive-strength = <16>;
			bias-pull-up;
			output-high;
		};

		wake-n-pins {
	    	pins = "gpio104";
			function = "gpio";

		    drive-strength = <2>;
		    bias-pull-up;
	    };
	};

	sdc2_default_state: sdc2-default-state {
		clk-pins {
			pins = "sdc2_clk";
			bias-disable;
			drive-strength = <16>;
		};

		cmd-pins {
			pins = "sdc2_cmd";
			bias-pull-up;
			drive-strength = <16>;
		};

		data-pins {
			pins = "sdc2_data";
			bias-pull-up;
			drive-strength = <16>;
		};
	};

	sdc2_card_det_n: sd-card-det-n-state {
		pins = "gpio126";
		function = "gpio";
		bias-pull-up;
	};
};

